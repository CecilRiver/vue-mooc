ight: 700;\n      dd\n        color: #545c63;\n        font-size: 12px;\n        line-height: 24px;\n",{"version":3,"sources":["tips.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA2FA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"tips.vue","sourceRoot":"src/pages/course-detail","sourcesContent":["<template>\r\n  <div class=\"course-detail-tips\">\r\n    <!-- 学习情况 -->\r\n    <div v-if=\"lastest && lastest.percent > 0\" class=\"learn-info\">\r\n      <p>\r\n        <span class=\"percent\">已学习{{ lastest.percent || 0 }}%</span>\r\n        <span class=\"duration\">学习耗时{{ lastest.duration || 0 }}分钟</span>\r\n      </p>\r\n      <mooc-progress\r\n        :percentage=\"lastest.percent\"\r\n        :show-text=\"false\"\r\n        :height=\"20\"\r\n        color=\"#f01414\"\r\n      />\r\n      <p class=\"latest\">\r\n        上次学习至：{{ lastest.chapter }}\r\n      </p>\r\n    </div>\r\n<!--     \r\n    <div class=\"tips-btn\">\r\n      {{ lastest.percent > 0 ? '继续学习' : '加入课程' }}\r\n    </div> -->\r\n        \r\n    <div class=\"tips-btn\" @click = joinCourse>\r\n      {{ '加入课程' }}\r\n    </div>\r\n\r\n    <!-- 课程须知 -->\r\n    <div class=\"tips-content\">\r\n      <dl>\r\n        <dt>课程须知</dt>\r\n        <dd>1、对基础知识已经掌握。</dd>\r\n        <dd>2、对复杂知识简单了解。</dd>\r\n      </dl>\r\n      <dl>\r\n        <dt>老师告诉你能学到什么？</dt>\r\n        <dd>什么都能学到 </dd>\r\n        <!-- <dd>2、理解Vue编程理念与直接操作Dom的差异</dd>\r\n        <dd>3、Vue常用的基础语法 </dd>\r\n        <dd>4、使用Vue编写TodoList功能 </dd>\r\n        <dd>5、什么是Vue的组件和实例 </dd>\r\n        <dd>6、Vue-cli脚手架工具的使用</dd>\r\n        <dd>7、但文件组件，全局样式与局部样式</dd> -->\r\n      </dl>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nimport { mapGetters } from 'vuex'\r\nimport axios from 'utils/axios.js'\r\n\r\nexport default {\r\n  computed: {\r\n    ...mapGetters(['userInfo'])\r\n  },\r\n  methods:{\r\n    joinCourse(){\r\n  const userInfo = this.$store.getters.userInfo;\r\n  console.log(\"加入啦\")\r\n  const courseSubscriptions = {\r\n    userId: userInfo[0].userId,\r\n    courseId:this.lastest.courseId,\r\n    subscriptionDate : new Date()\r\n  }\r\n  // 设置axios的baseURL\r\n    axios.defaults.baseURL = 'http://127.0.0.1:7000/';\r\n    axios.defaults.withCredentials = false;// Cookie跨域\r\n\r\n    console.log(courseSubscriptions)\r\n    axios.post('/course/coursesubscriptions/save',courseSubscriptions).\r\n    then(response => {\r\n      console.log(response)\r\n      this.$message.success(`加入课程：${this.lastest.courseName} 成功!`)\r\n    })\r\n   }\r\n  },\r\n\r\n\r\n\r\n\r\n  props: {\r\n    lastest: {\r\n      type: Object,\r\n      default () {\r\n        return {}\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>\r\n<style lang=\"stylus\" scoped>\r\n  .course-detail-tips\r\n    padding: 24px 32px 32px;\r\n    background-color: #fff;\r\n    box-shadow: 0 8px 16px 0 rgba(7,17,27,.1);\r\n    border-radius: 12px;\r\n    .learn-info\r\n      padding-bottom: 12px;\r\n      font-size: 12px;\r\n      color: #545c63;\r\n      & > p\r\n        margin-bottom: 9px;\r\n        line-height: 24px;\r\n        &.latest\r\n          margin-top: 24px;\r\n          line-height: 1;\r\n        .percent\r\n          font-size: 14px;\r\n          font-weight: 700;\r\n        .duration\r\n          float: right;\r\n    .tips-btn\r\n      padding: 11px 32px;\r\n      border-radius: 24px;\r\n      background-color: #f20d0d;\r\n      text-align: center;\r\n      line-height: 24px;\r\n      font-size: 16px;\r\n      color: #fff;\r\n      cursor: pointer;\r\n      &:hover\r\n        background-color: #c20a0a;\r\n    .tips-content\r\n      dl\r\n        margin-top: 24px;\r\n        dt\r\n          margin-bottom: 8px;\r\n          font-size: 14px;\r\n          font-weight: 700;\r\n        dd\r\n          color: #545c63;\r\n          font-size: 12px;\r\n          line-height: 24px;\r\n</style>\r\n"]}]}