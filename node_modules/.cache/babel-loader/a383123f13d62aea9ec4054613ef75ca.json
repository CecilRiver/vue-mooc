oc-dialog\r\n      title=\"消息中心设置\"\r\n      :visible.sync=\"dialogVisible\"\r\n      width=\"45%\"\r\n      top=\"20vh\"\r\n    >\r\n      <dl\r\n        v-for=\"(item,index) in list\"\r\n        :key=\"index\"\r\n        class=\"switch-group\"\r\n      >\r\n        <dt class=\"switch-title-box\">\r\n          <mooc-divider position=\"left\" :left=\"0\" color=\"#d0d6d9\">\r\n            {{ item.type }}\r\n          </mooc-divider>\r\n        </dt>\r\n        <dd\r\n          v-for=\"(subItem, index) in item.data\"\r\n          :key=\"index\"\r\n        >\r\n          <mooc-switch v-model=\"subItem.value\" active-color=\"#f01414\" inactive-color=\"#9199a1\" />{{ subItem.label }}\r\n        </dd>\r\n      </dl>\r\n      <div slot=\"footer\">\r\n        <mooc-button @click=\"dialogVisible=false\">\r\n          关闭\r\n        </mooc-button>\r\n        <mooc-button type=\"primary\" @click=\"dialogVisible=false\">\r\n          保存\r\n        </mooc-button>\r\n      </div>\r\n    </mooc-dialog>\r\n  </div>\r\n</template>\r\n<script>\r\nexport default {\r\n  props: {\r\n    list: {\r\n      type: Array,\r\n      default () {\r\n        return []\r\n      }\r\n    },\r\n    visible: {\r\n      type: Boolean,\r\n      default: false\r\n    }\r\n  },\r\n  computed: {\r\n    dialogVisible: {\r\n      get () {\r\n        return this.visible\r\n      },\r\n      set (val) {\r\n        this.$emit('update:visible', val)\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>\r\n<style lang=\"stylus\" scoped>\r\n  @import '~assets/stylus/variables.styl';\r\n  .notice-setting\r\n    .mooc-dialog\r\n      .mooc-dialog-body\r\n        padding: 15px 20px;\r\n        .switch-group\r\n          .switch-title-box\r\n            position: relative;\r\n            margin-bottom: 20px;\r\n          dd\r\n            display: inline-block;\r\n            margin-bottom: 20px;\r\n            padding: 0 20px;\r\n            width: 50%;\r\n            box-sizing: border-box;\r\n            .mooc-switch\r\n   