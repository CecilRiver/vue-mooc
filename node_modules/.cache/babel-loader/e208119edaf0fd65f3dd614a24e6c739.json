ndlePaginationChange(page) {\n      this.page = page;\n      this.getReadListData();\n    },\n    // 获取专栏类型数据\n    getReadTypes: function getReadTypes() {\n      var _this = this;\n\n      _getReadTypes().then(function (res) {\n        var code = res.code,\n            data = res.data,\n            msg = res.msg;\n\n        if (code === ERR_OK) {\n          data.unshift({\n            value: '全部'\n          });\n          _this.typeList = data;\n        } else {\n          _this.typeList = [];\n\n          _this.$message.error(msg);\n        }\n      }).catch(function () {\n        _this.typeList = [];\n      });\n    },\n    // 获取专栏列表数据\n    getReadListData: function getReadListData() {\n      var _this2 = this;\n\n      var params = {\n        page: this.page,\n        type: this.currentType\n      };\n      getReadList(params).then(function (res) {\n        var code = res.code,\n            data = res.data,\n            msg = res.msg;\n\n        if (code === ERR_OK) {\n          _this2.readList = data.list;\n          _this2.total = data.total;\n        } else {\n          _this2.readList = [];\n          _this2.total = 0;\n\n          _this2.$message.error(msg);\n        }\n      }).catch(function () {\n        _this2.readList = [];\n        _this2.total = 0;\n      });\n    }\n  },\n  computed: {\n    currentType: function currentType() {\n      if (this.typeList.length === 0) {\n        return '';\n      }\n\n      var type = this.typeList[this.currentIndex].value;\n      return type === '全部' ? '' : type;\n    }\n  },\n  components: {\n    Pagination: Pagination,\n    Empty: Empty\n  }\n};",{"version":3,"sources":["index.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA0EA,OAAA,UAAA,MAAA,sCAAA;AACA,OAAA,KAAA,MAAA,4BAAA;AACA,SAAA,YAAA,IAAA,aAAA,EAAA,WAAA,QAAA,aAAA;AACA,SAAA,MAAA,QAAA,eAAA;AACA,eAAA;AACA,EAAA,IADA,kBACA;AACA,WAAA;AACA,MAAA,QAAA,EAAA,EADA;AAEA,MAAA,YAAA,EAAA,CAFA;AAGA,MAAA,KAAA,EAAA,CAHA;AAIA