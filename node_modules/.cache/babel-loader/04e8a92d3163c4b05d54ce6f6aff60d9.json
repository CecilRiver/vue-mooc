/ 勾选事件\n    handleCheckClick: function handleCheckClick(course, index) {\n      course.isCheck = !course.isCheck;\n      this.$set(this.cartList, index, course);\n    },\n    // 删除事件\n    handleDeleteClick: function handleDeleteClick(item) {\n      var _this2 = this;\n\n      var params = {\n        id: item.id\n      };\n      deleteCart(params).then(function (res) {\n        var code = res.code,\n            msg = res.msg;\n\n        if (code === ERR_OK) {\n          _this2.$message.success(msg);\n\n          _this2.getCartListData();\n        } else {\n          _this2.$message.error(msg);\n        }\n      }).catch(function () {\n        _this2.$message.error('接口异常');\n      });\n    },\n    // 去结算\n    handleAccountClick: function handleAccountClick() {\n      if (this.isDisabled) {\n        return;\n      }\n\n      var checkCartList = this.cartList.filter(function (item) {\n        return item.isCheck;\n      });\n\n      if (checkCartList.length > 0) {\n        setCheckLessons(checkCartList);\n        this.$router.push('/cart/confirm');\n      } else {\n        this.$message.warning('请选择待结算的课程');\n      }\n    },\n    // 获取购物车列表接口数据\n    getCartListData: function getCartListData() {\n      var _this3 = this;\n\n      getCartList().then(function (res) {\n        var code = res.code,\n            data = res.data,\n            msg = res.msg;\n\n        if (code === ERR_OK) {\n          _this3.cartList = _this3.normalizeCartData(data);\n        } else {\n          _this3.cartList = [];\n\n          _this3.$message.error(msg);\n        }\n      }).catch(function () {\n        _this3.cartList = [];\n\n        _this3.$message.error('接口异常');\n      });\n    },\n    // 获取总计金额\n    getTotal: function getTotal() {\n      var list = this.cartList.slice();\n      var reuslt = 0;\n      list.forEach(function (item) {\n        if (item.isCheck) {\n          reuslt = reuslt + parseFloat(item.price);\n        }\n      });\n      return reuslt || 0;\n    },\n    // 处理购物车数据\n    normalizeCartData: function normalizeCartData(array) {\n      if (array.length === 0) {\n        return [];\n      }\n\n      array = array.map(function (item) {\n        return Object.assign({\n          isCheck: true\n        }, item);\n      });\n      return array;\n    }\n  },\n  computed: {\n    isCheckAll: function isCheckAll() {\n      return this.cartList.every(function (item) {\n        return item.isCheck;\n      });\n    },\n    checkNumber: function checkNumber() {\n      return this.cartList.filter(function (item) {\n        return item.isCheck;\n      }).length;\n    }\n  },\n  components: {\n    CartHeader: CartHeader,\n    Empty: Empty\n  }\n};",{"version":3,"sources":["cart.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA0EA,OAAA,UAAA;AACA,OAAA,KAAA,MAAA,4BAAA;AACA,SAAA,WAAA,EAAA,UAAA,QAAA,aAAA;AACA,SAAA,MAAA,QAAA,eAAA;AACA,SAAA,eAAA,QAAA,gBAAA;AACA,eAAA;AACA,EAAA,IADA,kBACA;AACA,WAAA;AACA,MAAA,QAAA,EAAA;AADA,KAAA;AAGA,GALA;AAMA,EAAA,OANA,qBAMA;AACA,SAAA,eAAA;AACA,GARA;AASA,EAAA,OAAA,EAAA;AACA;AACA,IAAA,mBAFA,iCAEA;AAAA;;AACA,UAAA,UAAA,GAAA,KAAA,UAAA;AACA,WAAA,QAAA,CAAA,OAAA,CAAA,UAAA,IAAA,EAAA,KAAA,EAAA;AACA,QAAA,IAAA,CAAA,OAAA,GAAA,CAAA,UAAA;;AACA,QAAA,KAAA,CAAA,IAAA,CAAA,KAAA,CAAA,QAAA,EAAA,KAAA,EAAA,IAAA;AACA,OAHA;AAIA,KARA;AASA;AACA,IAAA,gBAVA,4BAUA,MAVA,EAUA,KAVA,EAUA;AACA,MAAA,MAAA,CAAA,OAAA,GAAA,CAAA,MAAA,CAAA,OAAA;AACA,WAAA,IAAA,CAAA,KAAA,QAAA,EAAA,KAAA,EAAA,MAAA;AACA,KAbA;AAcA;AACA,IAAA,iBAfA,6BAeA,IAfA,EAeA;AAAA;;AACA,UAAA,MAAA,GAAA;AACA,QAAA,EAAA,EAAA,IAAA,CAAA;AADA,OAAA;AAGA,MAAA,UAAA,CAAA,MAAA,CAAA,CAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AAAA,YACA,IADA,GACA,GADA,CACA,IADA;AAAA,YACA,GADA,GACA,GADA,CACA,GADA;;AAEA,YAAA,IAAA,KAAA,MAAA,EAAA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,GAAA;;AACA,UAAA,MAAA,CAAA,eAAA;AACA,SAHA,MAGA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,GAAA;AACA;AACA,OARA,EAQA,KARA,CAQA,YAAA;AACA,QAAA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,MAAA;AACA,OAVA;AAWA,KA9BA;AA+BA;AACA,IAAA,kBA