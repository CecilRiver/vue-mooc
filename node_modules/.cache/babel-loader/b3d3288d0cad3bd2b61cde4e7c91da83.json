 }, {\n      id: 3,\n      title: '已完成',\n      status: 1\n    }, {\n      id: 4,\n      title: '已失效',\n      status: 2\n    }];\n    this.getOrderListData();\n  },\n  methods: {\n    // 导航点击\n    handleNavClick: function handleNavClick(index) {\n      this.currentIndex = index;\n      this.getOrderListData();\n    },\n    // 删除订单\n    handleDeleteClick: function handleDeleteClick(order) {\n      var _this = this;\n\n      this.$confirm('是否确认删除订单？', '提示', {\n        confirmButtonText: '确认',\n        cancelButtonText: '取消',\n        type: 'warning'\n      }).then(function () {\n        var params = {\n          id: order.id\n        };\n        deleteOrder(params).then(function (res) {\n          var code = res.code,\n              msg = res.msg;\n\n          if (code === ERR_OK) {\n            _this.$message.success(msg);\n\n            _this.getOrderListData();\n          } else {\n            _this.$message.error(msg);\n          }\n        }).catch(function () {\n          _this.$message.error('接口异常');\n        });\n      });\n    },\n    // 取消订单\n    handleCancelClick: function handleCancelClick(order) {\n      var _this2 = this;\n\n      this.$confirm('是否确认取消订单？', '提示', {\n        confirmButtonText: '确认',\n        cancelButtonText: '取消',\n        type: 'warning'\n      }).then(function () {\n        var params = {\n          id: order.id\n        };\n        cancelOrder(params).then(function (res) {\n          var code = res.code,\n              msg = res.msg;\n\n          if (code === ERR_OK) {\n            _this2.$message.success(msg);\n\n            _this2.getOrderListData();\n          } else {\n            _this2.$message.error(msg);\n          }\n        }).catch(function () {\n          _this2.$message.error('接口异常');\n        });\n      });\n    },\n    // 订单支付\n    handlePayClick: function handlePayClick(order) {\n      this.$router.push(\"/cart/pay/\".concat(order.code));\n    },\n    // 获取用户任务列表\n    getOrderListData: function getOrderListData() {\n      var _this3 = this;\n\n      // const params = {\n      //   page: this.page,\n      //   size: this.size,\n      //   status: this.currentStatus\n      // }\n      var userInfo = this.$store.getters.userInfo;\n      var params = {\n        userId: userInfo[0].userId,\n        type: this.currentStatus\n      };\n      getOrderList(params).then(function (res) {\n        var msg = res.msg,\n            code = res.code,\n            page = res.page;\n\n        if (code === ERR_OK) {\n          _this3.orderList = page.list;\n          _this3.total = page.totalCount;\n        } else {\n          _this3.orderList = [];\n\n          _this3.$message.error(msg);\n        }\n      }).catch(function () {\n        _this3.orderList = [];\n\n        _this3.$message.error('接口异常');\n      });\n    },\n    // 计算总额度\n    getTotal: function getTotal(list) {\n      if (!list || list.length === 0) {\n        return 0;\n      }\n\n      var total = 0;\n      list.forEach(function (item) {\n        if (item.isDiscount) {\n          total += parseFloat(Number(total + parseFloat(item.discountPrice)).toFixed(2));\n        } else {\n          total += parseFloat(Number(total + parseFloat(item.price)).toFixed(2));\n        }\n      });\n      return total;\n    }\n  },\n  computed: {\n    currentStatus: function currentStatus() {\n      return this.navList[this.currentIndex].title;\n    }\n  },\n  components: {\n    Pagination: Pagination,\n    Empty: Empty\n  }\n};",{"version":3,"sources":["list.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA8FA,OAAA,UAAA,MAAA,sCAAA;AACA,OAAA,KAAA,MAAA,4BAAA;AACA,SAAA,YAAA,EAAA,WAAA,EAAA,WAAA,QAAA,cAAA;AACA,SAAA,MAAA,QAAA,eAAA;AACA,eAAA;AACA,EAAA,IADA,kBACA;AACA,WAAA;AACA,MAAA,IAAA,EAAA,EADA;AAEA,MAAA,IAAA,EAAA,CAFA;AAGA,MAAA,KAAA,EAAA,CAHA;AAIA,MAAA,YAAA,EAAA,CAJA;AAKA,MAAA,OAAA,EAAA,EALA;AAMA,MAAA,SAAA,EAAA;AANA,KAAA;AAQA,GAVA;AAWA,EAAA,OAXA,qBAWA;AACA,SAAA,OAAA,GAAA,CACA;AA